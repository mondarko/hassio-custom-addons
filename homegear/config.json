{
  "name": "Homegear for Homematic",
  "version": "1.0",
  "slug": "homegear-homematic",
  "description": "Homegear for Homematic devices Hass.io add-on",
  "url": "https://github.com/mondarko/hassio-custom-addons",
  "startup": "before",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "boot": "auto",
  "gpio": true,
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "map": ["config:rw", "share:rw"],
  "ports": {
    "2001/tcp": 2001,
    "2002/tcp": 2002,
    "2003/tcp": 2003
  },
  "environment": {
  },
  "options": {
  },
  "schema": {
  },
  "devices": [
    "/dev/ttyAMA0:/dev/ttyAMA0:rwm",
    "/dev/ttyACM0:/dev/ttyACM0:rwm"
  ]
}
